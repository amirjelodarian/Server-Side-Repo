@model IEnumerable<Users>
@{
    ViewData["Title"] = "ContactUs";
}
<form action="" method="">

</form>
<table class="table table-responsive">
<h5>Count Of Users: @ViewBag.countUser</h5>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>email</th>
                <th>Password</th>
