@{
    ViewData["Title"] = "ContactUs";
}
<table class="table table-responsive">
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Family</th>
                <th>Gender</th>
                <th>Birthday</th>
            </tr>
            @{
                foreach(var item in Model){
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name</td>
                        <td>@item.Family</td>
                        <td>@item.Gender</td>
                        <td>@item.Birthday</td>
                    </tr>
                }
            }
</table>
<h1>تماس به ما</h1>
<p>09191234567</p>

<h2>@ViewBag.sayHi</h2>